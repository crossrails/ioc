{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Jasmine-Node Debugging",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceRoot}/node_modules/jasmine/bin/jasmine.js",
            "request": "launch",
            "type": "node",
            "preLaunchTask": "build",
            "internalConsoleOptions": "openOnSessionStart",
			"sourceMaps": true,
            "args": [
                "tests.js"
            ]
        }
    ]
}